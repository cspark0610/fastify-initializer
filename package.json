{
  "name": "fastify-intializer",
  "version": "1.0.1",
  "description": "microservicio para fastify-intializer",
  "apiReference": "fastify-intializer-api:1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_CONFIG_ENV=dev ENVIRONMENT=LOCAL nodemon exec",
    "tst": "cross-env NODE_CONFIG_ENV=tst ENVIRONMENT=LOCAL nodemon exec",
    "stg": "cross-env NODE_CONFIG_ENV=stg ENVIRONMENT=LOCAL nodemon exec",
    "start": "node ./dist/main.js",
    "test": "jest --detectOpenHandles",
    "depcheck": "depcheck .",
    "coverage": "jest --reporter=clover --reporter=html --coverage --detectOpenHandles --maxWorkers 1 --collectCoverageFrom='./src/**'",
    "coverage-summary": "jest --reporter=clover --reporter=html --coverage --detectOpenHandles --maxWorkers 1 --reporters='summary' --json --collectCoverageFrom='./src/**'",
    "dependency-check": "depcheck .",
    "functional-test": "echo TODO: implement some kind of functional tests",
    "functional-test-standalone": "echo TODO: implement some kind of functional tests",
    "ci_lint": "yaml-validator .gitlab-ci.yml",
    "build": "webpack",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "docker:build": "docker build -t mailing-address-ms .",
    "docker:run": "docker run --rm -it -p 3000:3000/tcp fastify-intializer:latest",
    "prepare": "husky || true",
    "postinstall": "force-resolutions"
  },
  "dependencies": {
    "@fastify/env": "^5.0.1",
    "@fastify/helmet": "^13.0.0",
    "@fastify/sensible": "^6.0.1",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.2.0",
    "close-with-grace": "^1.2.0",
    "config": "^3.3.9",
    "dependency-check": "^4.1.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.1.0",
    "fastify-plugin": "4.5.1",
    "force-resolutions": "^1.0.11",
    "joi-compiler": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash.defaultsdeep": "^4.6.1",
    "make-promises-safe": "^5.1.0",
    "undici": "^7.3.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/config": "3.3.1",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.17.15",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.51.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "nodemon": "^3.0.1",
    "pino-pretty": "9.1.1",
    "prettier": "^3.0.3",
    "sinon": "^19.0.2",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "keywords": [
    "template"
  ],
  "author": "Carlos S Park",
  "license": "ISC",
  "engines": {
    "node": "^20"
  },
  "resolutions": {
    "yaml": "2.2.2"
  }
}
